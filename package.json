{
  "name": "ci-tests",
  "version": "0.1.0",
  "description": "This project can be used to test the CI system integrations for cvrg.report",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha --timeout=3000",
    "coverage-from-stdin": "bash -c 'nyc report --reporter=text-lcov | bash <(curl -s -o- -L https://raw.githubusercontent.com/cvrg-report/cvrg/master/cvrg) || exit 0'",
    "coverage-from-reports": "nyc report --reporter=text-lcov && ./cvrg -t 123456abcdef"
  },
  "keywords": [
    "ci",
    "coverage",
    "reporting"
  ],
  "author": "tobilg@gmail.com",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cvrg-report/ci-tests.git"
  },
  "engines": {
    "node": ">=6.10 <=6.12"
  }
}
